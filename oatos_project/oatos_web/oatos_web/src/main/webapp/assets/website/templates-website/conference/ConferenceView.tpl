<div class="conference_view">
	<div class="page-header custom-project-title">
		<span class="left">视频会议</span>
        <a class="right" href="#" class="btn btn-link"><i class="icon-remove" style="font-size:16px;"></i></a>
	</div>
	<div class="operate-panel">
		<div class="btn-group">
			<button data-toggle="dropdown" class="btn dropdown-toggle">添加<span class="caret"></span></button>
			<ul class="dropdown-menu">
				<li><a href="#">预约会议</a></li>
				<li><a href="#">即时会议</a></li>
			</ul>
		</div>
		<button class="btn">删除</button>
		<div class="input-append">
			<input class="input-small"   type="text">
			<button class="btn" type="button">Search</button>
		</div>
	</div>
	<ul id='conference-list'>
		<li>主题</li>
	</ul>
</div>
<div class="add_now_conference conference_hide">
	<div class="page-header">
		<a href="#" class="btn btn-link">关闭</a>
		<h3>新建即时会议</h3>
	</div>
	<div class="operate-panel">
        <fieldset>
          <p>会议主题：</p>
          <input type="text" placeholder="本次会议标题">
          <p>参会人员：</p>
          <input type="text" id="addMember" placeholder="请输入或添加参加会议人员">
          <button class="btn addMember">添加</button>
          <p>会议资料：</p>
          <button class="btn">添加</button>
          <div>
              <label class="checkbox">
                <input type="checkbox"> 录制视频
              </label>
          </div>
          <button class="btn">确定</button>
          <button class="btn">取消</button>
        </fieldset>
	</div>
</div>
<div class="add_plan_conference conference_hide">
	<div class="page-header">
		<a href="#" class="btn btn-link">关闭</a>
		<h3>新建预约会议</h3>
	</div>
	<div class="operate-panel">
        <fieldset>
          <p>会议主题：</p>
          <input type="text" placeholder="本次会议标题">
          <p>会议日期：</p>
          <div data-provide="datepicker" data-date-format="yyyy-mm-dd" data-date="0d" class="input-append date datepicker">
          	<input type="text" readonly="" value="" size="16"><span class="add-on"><i class="icon-th"></i></span>
          </div>
          <div><input type="text">:<input type="text"></div>
          <p>预计时长：</p>
          <input type="text">时<input type="text">分
          <textarea rows="4" placeholder="会议概要"></textarea>
          <p>参会人员：</p>
          <input type="text" placeholder="请输入或添加参加会议人员">
          <button class="btn">添加</button>
          <p>会议资料：</p>
          <button class="btn">添加</button>
          <div>
              <label class="checkbox">
                <input type="checkbox"> 录制视频
              </label>
          </div>
          <button class="btn">确定</button>
          <button class="btn">取消</button>
        </fieldset>
	</div>
</div>