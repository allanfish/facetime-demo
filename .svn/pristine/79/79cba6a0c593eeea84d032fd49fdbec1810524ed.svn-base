    <link rel="stylesheet" type="text/css" href="assets/website/css/login-register.css">
  </head>

  <body>
    <div class="navbar navbar-inverse navbar-static-top">
      <div class="navbar-inner">
        <div class="container">
          <div class="brand">
            <span id="showReg"><a href="#">注册</a></span>
            <span id="showLog"><a href="#">登陆</a></span>
          </div>
          <div class="nav-collapse collapse">
            <ul class="nav row">
              <li class="span2"><a href="#" class="nav-logo"></a></li>
              <li class="span1 offset2"><a href="#">产品介绍</a></li>
              <li class="span1"><a href="#">私有云</a></li>
